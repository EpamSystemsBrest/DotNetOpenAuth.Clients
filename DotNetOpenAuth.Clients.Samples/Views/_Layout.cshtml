<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

</head>
<body>
    <div class="text-center">
        <h2>Sing in with </h2> @*Dirty: ctrl+c ctrl+v*@
        <a href="@Url.Action("Index", "Login", new { provider = "twitter" })" class="btn btn-social-icon btn-twitter white"><i class="fa fa-twitter"></i></a>
        <a href="@Url.Action("Index", "Login", new { provider = "googleplus" })" class="btn btn-social-icon btn-google-plus white"><i class="fa fa-google-plus"></i></a>
        <a href="@Url.Action("Index", "Login", new { provider = "flickr" })" class="btn btn-social-icon btn-flickr white"><i class="fa fa-flickr"></i></a>
        <a href="@Url.Action("Index", "Login", new { provider = "pinterest" })" class="btn btn-social-icon btn-pinterest white"><i class="fa fa-pinterest"></i></a>
        <a href="@Url.Action("Index", "Login", new { provider = "tumblr" })" class="btn btn-social-icon btn-tumblr white"><i class="fa fa-tumblr"></i></a>
        <a href="@Url.Action("Index", "Login", new { provider = "vk" })" class="btn btn-social-icon btn-vk white"><i class="fa fa-vk"></i></a>
        <a href="@Url.Action("Index", "Login", new { provider = "facebook" })" class="btn btn-social-icon btn-facebook white"><i class="fa fa-facebook"></i></a>
        <a href="@Url.Action("Index", "Login", new { provider = "instagram" })" class="btn btn-social-icon btn-instagram white"><i class="fa fa-instagram"></i></a>
        <a href="@Url.Action("Index", "Login", new { provider = "linkedin" })" class="btn btn-social-icon btn-linkedin white"><i class="fa fa-linkedin"></i></a>
        @RenderBody()
    </div>

    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/font-awesome.min.css" rel="stylesheet">
    <link href="~/Content/Custom/bootstrap-social.css" rel="stylesheet">
    <link href="~/Content/Custom/Custom.css" rel="stylesheet" />

    <script src="~/Scripts/jquery-2.1.1.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>
